{
"version": 2,
"builds": [
  {
    "src": "./public/index.html",
    "use": "@vercel/node"
  }
],
"routes": [
  {
    "src": "/(.*)",
    "dest": "./public/index.html",
    "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
    "headers": {
      "Access-Control-Allow-Origin": "*"
    }
  }
]
}



